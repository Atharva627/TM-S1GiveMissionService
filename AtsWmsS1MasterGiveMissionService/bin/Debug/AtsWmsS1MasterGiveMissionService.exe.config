<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
      <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
    </configSections>
    <connectionStrings>
        <add name="AtsWmsS1MasterGiveMissionService.Properties.Settings.ats_tata_metallics_dbConnectionString"
            connectionString="Data Source=localhost;Initial Catalog=ats_tata_metallics_db;User ID=ats-india;Password=Ats123*"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

  <appSettings>
    <!--<add key="IP_ADDRESS" value="10.255.20.27"/>-->
    <add key="IP_ADDRESS" value="172.16.67.60"/>
  </appSettings>

  <log4net>

    <!--1. service class-->
    <appender name="AtsWmsS1MasterGiveMissionServiceDebug" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="D:\\ATS_WMS_LOGS_ROOT\\AtsWmsS1MasterGiveMissionService\\AtsWmsS1MasterGiveMissionService\\Debug\\AtsWmsS1MasterGiveMissionServiceDebug.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Composite"/>
      <datePattern value="dd-MMMM-yyyy"/>
      <maxSizeRollBackups value="100"/>
      <maxDateRollBackups value="3"/>
      <maximumFileSize value="50MB"/>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="DEBUG"/>
        <levelMax value="DEBUG"/>
      </filter>
      <filter class="log4net.Filter.DenyAllFilter"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline"/>
      </layout>
    </appender>
    <appender name="AtsWmsS1MasterGiveMissionServiceError" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="D:\\ATS_WMS_LOGS_ROOT\\AtsWmsS1MasterGiveMissionService\\AtsWmsS1MasterGiveMissionService\\Error\\AtsWmsS1MasterGiveMissionServiceError.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Composite"/>
      <datePattern value="dd-MMMM-yyyy"/>
      <maxSizeRollBackups value="100"/>
      <maxDateRollBackups value="3"/>
      <maximumFileSize value="50MB"/>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR"/>
        <levelMax value="ERROR"/>
      </filter>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline"/>
      </layout>
    </appender>

    <logger name="AtsWmsS1MasterGiveMissionService">
      <appender-ref ref="AtsWmsS1MasterGiveMissionServiceError"/>
      <appender-ref ref="AtsWmsS1MasterGiveMissionServiceDebug"/>
    </logger>



    <!--2. Business class-->
    <appender name="AtsWmsS1MasterGiveMissionServiceDetailsDebug" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="D:\\ATS_WMS_LOGS_ROOT\\AtsWmsS1MasterGiveMissionService\\AtsWmsS1MasterGiveMissionServiceDetails\\Debug\\AtsWmsS1MasterGiveMissionServiceDetailsDebug.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Composite"/>
      <datePattern value="dd-MMMM-yyyy"/>
      <maxSizeRollBackups value="100"/>
      <maxDateRollBackups value="3"/>
      <maximumFileSize value="50MB"/>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="DEBUG"/>
        <levelMax value="DEBUG"/>
      </filter>
      <filter class="log4net.Filter.DenyAllFilter"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline"/>
      </layout>
    </appender>
    <appender name="AtsWmsS1MasterGiveMissionServiceDetailsError" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="D:\\ATS_WMS_LOGS_ROOT\\AtsWmsS1MasterGiveMissionService\\AtsWmsS1MasterGiveMissionServiceDetails\\Error\\AtsWmsS1MasterGiveMissionServiceDetailsError.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Composite"/>
      <datePattern value="dd-MMMM-yyyy"/>
      <maxSizeRollBackups value="100"/>
      <maxDateRollBackups value="3"/>
      <maximumFileSize value="50MB"/>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR"/>
        <levelMax value="ERROR"/>
      </filter>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline"/>
      </layout>
    </appender>

    <logger name="AtsWmsS1MasterGiveMissionServiceDetails">
      <appender-ref ref="AtsWmsS1MasterGiveMissionServiceDetailsError"/>
      <appender-ref ref="AtsWmsS1MasterGiveMissionServiceDetailsDebug"/>
    </logger>

    <root>
      <level value="ALL"/>
      <appender-ref ref="FileAppender"/>
      <appender-ref ref="ConsoleAppender"/>
      <appender-ref ref="RollingFileAppender"/>
    </root>

  </log4net>
  
</configuration>